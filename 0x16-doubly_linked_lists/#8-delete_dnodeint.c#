int delete_dnodeint_at_index(dlistint_t **head, unsigned int index)
{
	dlistint_t *head_ref;
	dlistint_t *delete;
	unsigned int i;

	head_ref = *head;
	for (i = 0; i < index && head_ref != NULL; i++)
		head_ref = head_ref->next;
	if (*head == head_ref)
	{
		*head = (*head)->next;
		(*head)->prev = NULL;
		free(head_ref);
		return (1);
	}
	if (i == index && head_ref->next != NULL)
		free(head_bef);
		head_aft = head_aft->next;
		if (head_aft == NULL)
			head_aft->next = NULL;
	}

}